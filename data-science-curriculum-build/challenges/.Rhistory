df_end %>%
group_by(county) %>%
filter(population < quantile(population, 0.25, na.rm = TRUE)) %>%
filter(county != "Loving") #filtered for better axes
df_end %>%
ggplot(aes(x = cases, y = deaths_per100k, color = population)) +
geom_point(alpha = 0.7)
